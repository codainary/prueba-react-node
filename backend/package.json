{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "dotenv -e .env.test jest --watch",
    "test:coverage": "dotenv -e .env.test jest --coverage",
    "analyze-deps": "madge --circular src",
    "visualize-deps": "madge --image dependency-graph.svg src",
    "seed": "node prisma/seed.js",
    "migrate": "dotenv -e .env -- prisma migrate deploy && npm run seed",
    "migrate-dev": "dotenv -e .env.development -- prisma migrate dev && npm run seed",
    "migrate-test": "dotenv -e .env.test -- prisma migrate deploy && npm run seed",
    "dev": "nodemon src/server.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/*.test.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.18.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "dotenv-cli": "^7.4.2",
    "faker": "^6.6.6",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
