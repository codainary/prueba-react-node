{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "semi": ["error", "never"],
        "prettier/prettier": [
        "error",
        {
            "semi": false
        }
        ],
        "no-unused-vars": ["warn"],
        "eqeqeq": ["error", "always"],
        "no-console": ["warn"]
    },
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
}
}